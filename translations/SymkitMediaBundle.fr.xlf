<?xml version="1.0" encoding="UTF-8"?>
<xliff version="1.2" xmlns="urn:oasis:names:tc:xliff:document:1.2">
    <file source-language="en" target-language="fr" datatype="plaintext" original="SymkitMediaBundle">
        <body>
            <trans-unit id="admin.list.title"><source>Media Library</source><target>Médiathèque</target></trans-unit>
            <trans-unit id="admin.list.description"><source>Manage your uploaded files.</source><target>Gérez vos fichiers téléversés.</target></trans-unit>
            <trans-unit id="admin.create.title"><source>Upload Media</source><target>Envoyer un média</target></trans-unit>
            <trans-unit id="admin.create.description"><source>Add a new file to the library.</source><target>Ajouter un nouveau fichier à la médiathèque.</target></trans-unit>
            <trans-unit id="admin.edit.title"><source>Edit Media</source><target>Modifier le média</target></trans-unit>
            <trans-unit id="admin.edit.description"><source>Update file information.</source><target>Mettre à jour les informations du fichier.</target></trans-unit>
            <trans-unit id="form.file_section.label"><source>File</source><target>Fichier</target></trans-unit>
            <trans-unit id="form.file_section.description"><source>Upload or replace the media file.</source><target>Envoyer ou remplacer le fichier média.</target></trans-unit>
            <trans-unit id="form.general.label"><source>General</source><target>Général</target></trans-unit>
            <trans-unit id="form.general.description"><source>Meta information about the media.</source><target>Informations générales sur le média.</target></trans-unit>
            <trans-unit id="form.alt_text.label"><source>Alternative Text</source><target>Texte alternatif</target></trans-unit>
            <trans-unit id="form.alt_text.help"><source>Description for accessibility (alt tag).</source><target>Description pour l'accessibilité (balise alt).</target></trans-unit>
            <trans-unit id="form.placeholder"><source>Drag and drop a file or click to upload</source><target>Glissez-déposez un fichier ou cliquez pour envoyer</target></trans-unit>
            <trans-unit id="form.media_file.label"><source>Media File</source><target>Fichier média</target></trans-unit>
            <trans-unit id="form.mime_types_message"><source>Please upload a valid file</source><target>Veuillez envoyer un fichier valide</target></trans-unit>
            <trans-unit id="transformer.media_not_found"><source>Media with id "%id%" not found</source><target>Média avec l'id « %id% » introuvable</target></trans-unit>
            <trans-unit id="api.error.no_file"><source>No file uploaded</source><target>Aucun fichier envoyé</target></trans-unit>
            <trans-unit id="api.error.upload_failed"><source>Upload failed. Please try again.</source><target>L'envoi a échoué. Veuillez réessayer.</target></trans-unit>
            <trans-unit id="api.error.security"><source>This file was rejected for security reasons.</source><target>Ce fichier a été refusé pour des raisons de sécurité.</target></trans-unit>
            <trans-unit id="search.category"><source>Media</source><target>Médias</target></trans-unit>
            <trans-unit id="library.drop_file"><source>Drop file to upload</source><target>Déposez le fichier pour l'envoyer</target></trans-unit>
            <trans-unit id="library.search_placeholder"><source>Search media...</source><target>Rechercher un média...</target></trans-unit>
            <trans-unit id="library.upload"><source>Upload</source><target>Envoyer</target></trans-unit>
            <trans-unit id="library.no_media"><source>No media found</source><target>Aucun média trouvé</target></trans-unit>
            <trans-unit id="library.no_matches"><source>No matches for "%query%". Try distinct search terms.</source><target>Aucun résultat pour « %query% ». Essayez d'autres termes.</target></trans-unit>
            <trans-unit id="library.get_started"><source>Get started by uploading a new image.</source><target>Commencez par envoyer une nouvelle image.</target></trans-unit>
            <trans-unit id="library.pagination_aria"><source>Pagination</source><target>Pagination</target></trans-unit>
            <trans-unit id="library.previous"><source>Previous</source><target>Précédent</target></trans-unit>
            <trans-unit id="library.next"><source>Next</source><target>Suivant</target></trans-unit>
            <trans-unit id="library.showing_results"><source>Showing %from% to %to% of %total% results</source><target>Affichage de %from% à %to% sur %total% résultats</target></trans-unit>
            <trans-unit id="library.delete_confirm"><source>Are you sure you want to delete this item?</source><target>Êtes-vous sûr de vouloir supprimer cet élément ?</target></trans-unit>
            <trans-unit id="picker.replace"><source>Replace</source><target>Remplacer</target></trans-unit>
            <trans-unit id="picker.remove"><source>Remove</source><target>Supprimer</target></trans-unit>
            <trans-unit id="picker.select_image"><source>Select an image</source><target>Sélectionner une image</target></trans-unit>
            <trans-unit id="picker.select_media"><source>Select Media</source><target>Sélectionner un média</target></trans-unit>
            <trans-unit id="picker.close"><source>Close</source><target>Fermer</target></trans-unit>
        </body>
    </file>
</xliff>
